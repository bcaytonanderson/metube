<h1><%= @movie.title%></h1>

<h3><%= @movie.subtitle%></h3>

<iframe width="420" height="315" src=<%= @movie.video_id %> frameborder="0" allowfullscreen></iframe>

<ul>
<% @all.each do |x| %>
  <li>
    <a href="/movies/<%= x.id %>"><%= x.title %></a>
  </li>
<% end %>
</ul>

<p><%= @movie.description %></p>

